    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-verbose-reporter": "0.0.6",
    "rimraf": "^2.6.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.18.0",
    "typescript": "~4.6.2",
    "eslint-plugin-import": "2.27.5",
